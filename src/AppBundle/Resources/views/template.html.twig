{% extends '::base.html.twig' %} 

{% block stylesheets %} 
{% stylesheets 
	'bundles/app/css/design.css' 
	'bundles/app/css/widget.css' 
	'bundles/app/css/themes.css' 
	filter='cssrewrite' %}
<link rel="stylesheet" href="{{ asset_url }}" />
{% endstylesheets %}
{% endblock %} 

{% block body %} 
{% include 'AppBundle:Navigation:default_nav.html.twig' %}

{% block header %}
<div class="intro-header">
	<div class="container">
		<div class="row">
			<div class="col-lg-12">
				<div class="intro-message">
					<h1>Brände Pokernight</h1>
					<h3>bla bla</h3>
					<hr class="intro-divider">
					<ul class="list-inline intro-social-buttons">
						<li><a class="btn btn-default btn-lg"
							href="https://twitter.com/SBootstrap"><i
								class="fa fa-twitter fa-fw"></i> <span class="network-name">Twitter</span></a>
						</li>
						<li><a class="btn btn-default btn-lg"
							href="https://github.com/IronSummitMedia/startbootstrap"><i
								class="fa fa-github fa-fw"></i> <span class="network-name">Github</span></a>
						</li>
						<li><a class="btn btn-default btn-lg" href="#"><i
								class="fa fa-linkedin fa-fw"></i> <span class="network-name">Linkedin</span></a>
						</li>
					</ul>
				</div>
			</div>
		</div>
		{# --- row #}
	</div>
	{# --- container#}
</div>
{# --- header #} 
{% endblock header %} 

{% block content %} {% endblock%}

{% block banner %}
<div class="banner">
	<div class="container">
		<div class="row">
			<div class="col-lg-6">
				<h2>Connect to Start Bootstrap:</h2>
			</div>
			<div class="col-lg-6">
				<ul class="list-inline banner-social-buttons">
					<li><a class="btn btn-default btn-lg"
						href="https://twitter.com/SBootstrap"><i
							class="fa fa-twitter fa-fw"></i> <span class="network-name">Twitter</span></a>
					</li>
					<li><a class="btn btn-default btn-lg"
						href="https://github.com/IronSummitMedia/startbootstrap"><i
							class="fa fa-github fa-fw"></i> <span class="network-name">Github</span></a>
					</li>
					<li><a class="btn btn-default btn-lg" href="#"><i
							class="fa fa-linkedin fa-fw"></i> <span class="network-name">Linkedin</span></a>
					</li>
				</ul>
			</div>
		</div>

	</div>
	<!-- /.container -->
</div>
{% endblock banner %}

